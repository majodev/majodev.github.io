<div class="block-noteLatest">
  <h2 class="linedSeparator">{{!-- <i class="fa fa-puzzle-piece"></i>&nbsp;&nbsp; --}}Recent notes</h2>
  {{#if notes}}
    {{#each_upto notes 3}}
      {{!-- {{> noteArticleItem}} --}}
      <div class="row">
        <div class="col-md-offset-1 col-md-10 col-sm-12">
        {{> noteItemSmall}}
        </div>
      </div>
    {{/each_upto}}
  {{else}}
    <p>Sorry, no notes were published.</p>
  {{/if}}
</div>