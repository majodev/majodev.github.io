<div class="tagCloud">
  {{#each _sortedTags}}
    <a href="#{{tag}}" style="font-size: {{multiply 7 children.length }}px">#{{tag}}</a>
  {{/each}}
</div>

<div class="tags">
  {{#each _sortedTags}}
    <h3 id="{{tag}}">#{{tag}}</h3>
    {{#withSortReverse children "date"}}
      {{> noteItem}}
    {{/withSortReverse}}
  {{/each}}
</div>