{
  "inject": {
    "css": [],
    "less": {
      "dirs": [
        "support/less/includes",
        "support/less/bootstrap",
        "bower_components/bootstrap/less",
        "bower_components/lato/less",
        "bower_components/font-awesome/css",
        "bower_components/highlightjs/src/styles"
      ],
      "src": "support/less/style.less",
      "dest": "build/assets/css/style.css"
    },
    "js_head": [
      "bower_components/modernizr/modernizr.js"
    ],
    "js": [
      "bower_components/jquery/dist/jquery.js",
      "bower_components/velocity/jquery.velocity.js",
      "bower_components/fastclick/lib/fastclick.js",
      "bower_components/bootstrap/js/transition.js",
      "bower_components/bootstrap/js/collapse.js",
      "bower_components/history.js/scripts/bundled-uncompressed/html4+html5/jquery.history.js",
      "support/js/consoleErrors.js",
      "support/js/ajax.js",
      "support/js/enableFastClick.js",
      "support/js/consoleDisclaimer.js",
      "support/js/konamicode.js"
    ],
    "fonts": [
      "bower_components/font-awesome/fonts/fontawesome-webfont.*",
      "bower_components/lato/font/lato-*.*"
    ],
    "metalsmithBaseDir": {
      "css": "/assets/css/",
      "js": "/assets/js/"
    },
    "gruntTargetDir": {
      "css": "build/assets/css/",
      "js": "build/assets/js/",
      "fonts": "build/assets/fonts/"
    },
    "productive": {
      "css": "styles.min.css",
      "js": "scripts.min.js",
      "js_head": "scripts_head.min.js"
    }
  }
}